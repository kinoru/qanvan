html
body
    margin 0
    padding 0
    font 16px/2 sans-serif

#BoardList
    list-style none

    margin 0
    padding 0 20px
    position absolute
    top 0
    left 0
    width 160px
    height 100%

    background #369
    color #CCC

    li
        cursor pointer
        &:hover
            color #FFF

#CardListArea
    margin-left 200px

.CardList
    border 1px solid #CCC
    border-radius 3px
    padding 0 16px
    margin 12px
